/{
	vcc_lcd_5v: lcd-pwr-on {
		compatible = "regulator-fixed";
		regulator-name = "LCD_PWR_ON";
		regulator-min-microvolt = <5000000>;
		regulator-max-microvolt = <5000000>;
		gpio = <&gpio_expander 2 GPIO_ACTIVE_HIGH>;
		enable-active-high;
