&i2c8 {
	status = "okay";
	touchscreen: touchscreen@38 {
		compatible = "edt,edt-ft5506";
		reg = <0x38>;

		//reset-gpios = <&gpio_expander 1 GPIO_ACTIVE_LOW>;

		interrupt-parent = <&gpio2>;
		interrupts = <RK_PC5 IRQ_TYPE_EDGE_FALLING>;

		touchscreen-size-x = <1200>;
		touchscreen-size-y = <1920>;
		touchscreen-swapped-x-y;
		touchscreen-inverted-x;
		vcc-supply = <&vcc_lcd_5v>;
	};
};


#if J4_CSI_CAM
/{
	j4_cam_clk: j4-external-camera-clock {
		compatible = "fixed-clock";
		clock-frequency = <24000000>;
		clock-output-names = "J4_CLK_CAMERA_24MHZ";
		#clock-cells = <0>;
	};
};

&i2c2 {
	status = "okay";

	imx577@1a {
		status = "okay";
		compatible = "sony,imx577";
		reg = <0x1a>;

		power-domains = <&power RK3588_PD_VI>;

		/* CAM_OE  -> J1 pin3 -> GPIO1_B1 */
		/* CAM_SEL -> J1 pin5 -> GPIO1_B2 */

		clocks = <&j4_cam_clk>;
		clock-names = "xvclk";

		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "front";
		rockchip,camera-module-name = "NC";
		rockchip,camera-module-lens-name = "NC";
		port {
			j4_csi_cam_out: endpoint {
				remote-endpoint = <&csidcphy0_in>;
				data-lanes = <1 2 3 4>;
			};
		};
	};
};
#endif // J4_CSI_CAM end


#if J3_CSI_CAM
/{
	j3_cam_clk: j3-external-camera-clock {
		compatible = "fixed-clock";
		clock-frequency = <24000000>;
		clock-output-names = "J3_CLK_CAMERA_24MHZ";
		#clock-cells = <0>;
	};
};


&i2c4 {
	status = "okay";
	ov2718@36 {
		status = "okay";
		compatible = "ovti,ov2718";
		reg = <0x36>;
		power-domains = <&power RK3588_PD_VI>;
		clocks = <&j3_cam_clk>;
		clock-names = "xvclk";

		reset-gpios = <&gpio_expander 5 GPIO_ACTIVE_LOW>; // CAM_RSTN
		pwd-gpios = <&gpio_expander 6 GPIO_ACTIVE_HIGH>; // CAM_PWRDWN

		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "NC";
		rockchip,camera-module-lens-name = "NC";
		port {
			j3_csi_cam_out: endpoint {
				remote-endpoint = <&csidcphy1_in>;
				data-lanes = <1 2>;
			};
		};
	};
};
#endif // J3_CSI_CAM end
