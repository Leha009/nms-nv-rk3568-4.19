/*SMARC: I2C_GP*/
&i2c3{
	status = "okay";
	/*pinctrl-0 = <&i2c3m0_xfer_my>;*/
	pinctrl-0 = <&i2c3m0_xfer>;
	eeprom50: eeprom@50 {
		compatible = "atmel,24c02";
		reg = <0x50>;
	};

	D15_i2cmux@74 {
		compatible = "nxp,pca9546";
		reg = <0x74>;
		#address-cells = <1>;
		#size-cells = <0>;
		// vcc-supply = <&regulator_i2c_3v3>;
		// vcc-pullup-supply = <&regulator_i2c_3v3>;
		i2c-mux-idle-disconnect;
		I2C_MPCI: ch@0 { /*sfp20*/
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		I2C_GP_A: ch@1 { /*sfp21*/
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		I2C_DSI0: ch@2 { /*sfp22*/
			reg = <2>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
		I2C_DSI1: ch@3 { /*sfp23*/
			reg = <3>;
			#address-cells = <1>;
			#size-cells = <0>;
		};
	};

};
