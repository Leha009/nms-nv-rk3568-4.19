&pinctrl {
	cam {
		vcc_cam_en: vcc_cam_en {
			rockchip,pins = <1 RK_PB1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	display {
		vcc_mipi_en: vcc_mipi_en {
			rockchip,pins = <3 RK_PC0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	ethernet {
		eth_phy_rst: eth_phy_rst {
			rockchip,pins = <3 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	hym8563 {
		hym8563_int: hym8563-int {
			rockchip,pins = <0 RK_PD3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	leds {
		led_user_en: led_user_en {
			rockchip,pins = <2 RK_PC2 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	pcie {
		pcie_enable_h: pcie-enable-h {
			rockchip,pins = <0 RK_PD4 RK_FUNC_GPIO &pcfg_pull_none>;
		};

		pcie_reset_h: pcie-reset-h {
			rockchip,pins = <3 RK_PC1 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	pmic {
		pmic_int: pmic_int {
			rockchip,pins =
				<0 RK_PA3 RK_FUNC_GPIO &pcfg_pull_up>;
		};
	};

	usb {
		vcc5v0_usb_host_en: vcc5v0_usb_host_en {
			rockchip,pins = <0 RK_PA6 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc5v0_usb_hub_en: vcc5v0_usb_hub_en {
			rockchip,pins = <0 RK_PD5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		vcc5v0_usb_otg_en: vcc5v0_usb_otg_en {
			rockchip,pins = <0 RK_PA5 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};

	eth{
		gmac0_gpio: gmac0_gpio {
			rockchip,pins = <3 RK_PA7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		gmac1_gpio: gmac1_gpio {
			rockchip,pins = <3 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	hog {
		sata_usb3_mux_hog: sata-usb3-hog {
			rockchip,pins = <3 RK_PB5 RK_FUNC_GPIO &pcfg_pull_down>; // sata
			// rockchip,pins = <3 RK_PB5 RK_FUNC_GPIO &pcfg_pull_up>; // usb3
		};
		/* on SMARC pins  S139/S140: CAN(SMARC CAN1)  or i2c (I2C_LCD)*/
		i2c2_can_mux_hog: i2c2_can_hog {
			rockchip,pins = <3 RK_PD1 RK_FUNC_GPIO &pcfg_pull_down>; // can
			// rockchip,pins = <3 RK_PD1 RK_FUNC_GPIO &pcfg_pull_up>; // i2c
		};
		
		spi2uart_reset: spi2uart-reset{
			rockchip,pins =
				< 3 RK_PD3 RK_FUNC_GPIO &pcfg_pull_none>;
		};

	};
	lcd0 {
		lcd0_bklt_en_pins: lcd0-bklt-en {
			rockchip,pins =
				< 0 RK_PB0 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		lcd0_vdd_en_pins: lcd0-vdd-en{
			rockchip,pins =
				< 0 RK_PC7 RK_FUNC_GPIO &pcfg_pull_none>;
		};
	};
	rs422 {
		spi2uart_irq: spi2uart-irq {
			rockchip,pins =
				< 4 RK_PC3 RK_FUNC_GPIO &pcfg_pull_none>;
		};
		rs_a_fullduplex_mode: rs-a-fullduplex{
			rockchip,pins = < 3 RK_PD4 RK_FUNC_GPIO &pcfg_pull_up>; //RS422
			//rockchip,pins = < 3 RK_PD4 RK_FUNC_GPIO &pcfg_pull_down>; //RS485
		};
		rs_b_fullduplex_mode: rs-b-fullduplex{
			rockchip,pins = < 3 RK_PD2 RK_FUNC_GPIO &pcfg_pull_up>; //RS422
			//rockchip,pins = < 3 RK_PD2 RK_FUNC_GPIO &pcfg_pull_down>; //RS485
		};
	};
};

