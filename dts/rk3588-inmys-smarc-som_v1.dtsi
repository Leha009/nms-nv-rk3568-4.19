&gmac0 {
	clock_in_out = "output";
	phy-handle = <&rgmii_phy0>;
	//phy-mode = "rgmii-rxid";
	phy-mode = "rgmii-id";

	pinctrl-0 = <&gmac0_miim
		&gmac0_tx_bus2
		&gmac0_rx_bus2
		&gmac0_rgmii_clk
		&gmac0_rgmii_bus>;
	pinctrl-names = "default";
	rx_delay = <0x00>;
	tx_delay = <0x43>;
	status = "okay";

	/* MAC address stored in eeprom50 */
	nvmem-cells = <&gmac0_mac>;
	nvmem-cell-names = "mac-address";
};
